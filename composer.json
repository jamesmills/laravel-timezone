{
    "name": "jamesmills/laravel-timezone",
    "license": "MIT",
    "description": "Timezone storage and retrieval for Laravel",
    "keywords": [
        "laravel",
        "timezone"
    ],
    "authors": [
        {
            "name": "James Mills",
            "email": "james@clicksco.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "Illuminate/auth": "5.6.*|5.7.*|5.8.*|^6|^7",
        "Illuminate/support": "5.6.*|5.7.*|5.8.*|^6|^7",
        "torann/geoip": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "orchestra/testbench": "^3.6|^3.7|^3.8|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "JamesMills\\LaravelTimezone\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JamesMills\\LaravelTimezone\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamesMills\\LaravelTimezone\\LaravelTimezoneServiceProvider"
            ],
            "aliases": {
                "Timezone": "JamesMills\\LaravelTimezone\\Facades\\TimezoneFacade"
            }
        }
    },
    "scripts": {
        "pre-package-install": [

        ]
    }
}
