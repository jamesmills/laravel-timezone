{
    "name": "jamesmills/laravel-timezone",
    "license": "MIT",
    "description": "Timezone storage and retrieval for Laravel",
    "keywords": [
        "laravel",
        "timezone"
    ],
    "authors": [
        {
            "name": "James Mills",
            "email": "james@clicksco.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.6.* || 5.7.* || 5.8.* || ^6 || ^7",
        "torann/geoip": "^1.0",
        "nesbot/carbon": "^1.0 || ^2.0"
    },
    "autoload": {
        "psr-4": {
            "JamesMills\\LaravelTimezone\\": "src/",
            "JamesMills\\LaravelTimezone\\Database\\Seeds\\": "database/seeds/"

        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpunit/phpunit": "^8.5",
        "phing/phing": "^2.16",
        "pdepend/pdepend": "^2.6",
        "phploc/phploc": "^5.0",
        "phpmd/phpmd": "^2.8",
        "sebastian/phpcpd": "^4.1",
        "squizlabs/php_codesniffer": "^3.5",
        "symplify/easy-coding-standard": "^7.2",
        "object-calisthenics/phpcs-calisthenics-rules": "^3.7",
        "phpspec/phpspec": "^6.1"
    },
    "extra": {
        "laravel": {
            "providers": [
                "JamesMills\\LaravelTimezone\\Providers\\LaravelTimezoneServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": [
            "phing"
        ],
        "test:ci": [
            "phing qa"
        ],
        "git-hooks": [
            "rsync -av --exclude='*.md' ./githooks/ ./.git/hooks"
        ]
    }
}
